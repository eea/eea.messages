<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="eea">


  <browser:resource
    name="eea.messages.view.css"
    file="./css/view.css"
   />

  <browser:resource
    name="eea.messages.view.js"
    file="./js/view.js"
   />

  <browser:resourceDirectory
    name="eea.messages.images"
    directory="./img" />

</configure>
